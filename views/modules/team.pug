.team
  .container
    section.team__wrapper
      h2.section-title Наша команда
      ul.team__list.slider-list
        each item in teamList
          li.team__item
            .item__img
              img(src=item.photo)
            .item__name
              p #{item.content.firstname}
              p #{item.content.secondname}
            .item__prof
              p #{item.content.profession}
            a(class="btn item__btn" href=item.urlBtn) узнать больше
      .slider-container
